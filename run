export SERVICE_NAME=pathetic-java-web-app
export HONEYCOMB_METRICS_DATASET=mysterious-jvm-metrics
export OTEL_JAVAAGENT_EXTENSIONS=opentelemetry-java-instrumentation-extension-demo-1.0-all.jar
export OTEL_JAVAAGENT_DEBUG=true

./mvnw spring-boot:run \
  -Dspring-boot.run.jvmArguments="-javaagent:honeycomb-opentelemetry-javaagent-1.5.2.jar -Dotel.javaagent.extensions=opentelemetry-java-instrumentation-extension-demo-1.0-all.jar"
